 &userin
 idotitle=1,titlecolor='def.foreground',
 ptimes=0,3,24
 ptimeunits='h',tacc=120,timezone=-7,iusdaylightrule=1,
 iinittime=1,ifcsttime=1,ivalidtime=1,inearesth=0,
 flmin=.09, frmax=.92, fbmin=.10, ftmax=.85,
 ntextq=0,ntextcd=0,fcoffset=0.0,idotser=0,
 idescriptive=1,icgmsplit=0,maxfld=10,itrajcalc=0,imakev5d=0
 /
 &trajcalc
 rtim=15,ctim=6,dtfile=3600.,dttraj=600.,vctraj='s',
 xjtraj=95,90,85,80,75,70,65,80.6,80.6,80.6,80.6,80.6,80.6,
 yitraj=50,55,60,65,70,75,80,77,77,77,77,77,77,
 zktraj=25,25,25,25,25,25,25,27,25,22,19,16,13,
 ihydrometeor=0
 /
===========================================================================
----------------------    Plot Specification Table    ---------------------
===========================================================================
# vcmx  : Maximum vector value (use wind barbs if vcmx < 0)
feld=uuu,vvv; ptyp=hv; vcor=p; levs=500; vcmx=-30.; colr=red; intv=5
feld=uuu,vvv; ptyp=hv; vcor=p; levs=700; vcmx=-30.; colr=green; intv=5
feld=map; ptyp=hb
feld=tic; ptyp=hb
===========================================================================
# 700 to 500 mb vertical wind shear vectors
feld=ushp070050,vshp070050; ptyp=hv; vcmx=-30.; colr=blue; intv=5
feld=map; ptyp=hb
feld=tic; ptyp=hb
===========================================================================
# Remember vcor=z means to use height vertical coordinates
# This is NOT above ground level (AGL).
feld=uuu,vvv; ptyp=hv; vcor=z; levs=3.0; vcmx=30.; colr=red; intv=5
feld=uuu,vvv; ptyp=hv; vcor=z; levs=0.9; vcmx=30.; colr=green; intv=5
feld=map; ptyp=hb
feld=tic; ptyp=hb
===========================================================================
# The ushz and vshz fields expect the top and bottom
# levels to be given in hectometers (hm).
# e.g. ushz005030 is the u-component of the vertical wind shear
# from 9 hm (0.9 km) to 30 hm (3.0 km).
feld=ushz009030,vshz009030; ptyp=hv; vcmx=30.; colr=blue; intv=5
feld=map; ptyp=hb
feld=tic; ptyp=hb
===========================================================================
# same, but AGL
feld=usha009030,vsha009030; ptyp=hv; vcmx=30.; colr=blue; intv=5
feld=map; ptyp=hb
feld=tic; ptyp=hb
============================================================================
# horizonal wind vectors on model level index 15
feld=uuu,vvv; ptyp=hv; vcor=s; levs=15; vcmx=30.; colr=red; intv=5
feld=uuu,vvv; ptyp=hv; vcor=s; levs=23; vcmx=30.; colr=green; intv=5
feld=map; ptyp=hb
feld=tic; ptyp=hb
===========================================================================
# vertical wind shear from model level 23 to 15
feld=ushs023015,vshs023015; ptyp=hv; vcmx=30.; colr=blue; intv=5
feld=map; ptyp=hb
feld=tic; ptyp=hb
===========================================================================
feld=pvo; ptyp=vc; crsa=10,10; crsb=40,30; vcor=p; vwin=1050,200; cint=.25;>
   cmth=fill; cosq=0,white,4,dark.gray; cbeg=0; cend=5
feld=the; ptyp=vc; cint=2; colr=red
feld=uuu,vvv,omg; ptyp=vv
feld=tic; ptyp=vb
===========================================================================
feld=tic; ptyp=sb; sloc=CHH; hodo; sndg
feld=tmc; ptyp=sc; colr=red
feld=tdp; ptyp=sc; colr=blue
feld=uuu,vvv; ptyp=sv; colr=dark.green; hodo; sndg
===========================================================================
