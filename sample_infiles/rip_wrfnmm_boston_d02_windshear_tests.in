   47.5,g45dbz,52.5,g50dbz,57.5,g55dbz,62.5,g60dbz,67.5,g65dbz,>
   72.5,g70dbz,77.5,g75dbz
feld=map; ptyp=hb
feld=tic; ptyp=hb
